<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd">
	<system>
		<tab id="zoho" translate="label" sortOrder="999">
			<label>Zoho</label>
		</tab>
		<section id="crm" sortOrder="10" showInWebsite="1" showInStore="1" showInDefault="1" translate="label">
			<label>Crm </label>
			<tab>zoho</tab>
			<resource>Zoho_Crm::config_zoho_crm</resource>
			<group id="general" sortOrder="10" showInWebsite="1" showInStore="1" showInDefault="1" translate="label">
				<label>general</label>
				<field id="client_id" type="text" sortOrder="10" showInWebsite="1" showInStore="1" showInDefault="1" translate="label">
					<label>client_id</label>
					<comment/>
				</field>
				<field id="client_secret" type="text" sortOrder="10" showInWebsite="1" showInStore="1" showInDefault="1" translate="label">
					<label>client_secret</label>
					<comment/>
				</field>
				<field id="api_domain" type="text" sortOrder="10" showInWebsite="1" showInStore="1" showInDefault="1" translate="label">
					<label>api_domain</label>
					<comment/>
				</field>
				<field id="refresh_token" type="text" sortOrder="10" showInWebsite="1" showInStore="1" showInDefault="1" translate="label">
					<label>refresh_token</label>
					<comment/>
				</field>
			</group>
		</section>
	</system>
</config>
